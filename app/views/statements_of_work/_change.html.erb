<div class="change">
  <%= form_for @statement_of_work, url: @submit_path do |f| %>
      <div>
        <%= f.label 'Number' %>
        <%= f.text_field :number %>
      </div>
      <div>
        <%= f.label 'Purchase Order Number' %>
        <%= f.text_field :purchase_order_number %>
      </div>
      <div>
        <%= f.label 'Client' %>
        <%= f.text_field :client %>
      </div>
      <div>
        <%= f.label 'Nickname' %>
        <%= f.text_field :nickname %>
      </div>
      <div>
        <%= f.submit 'Save' %>
        <%= cancel_button %>
      </div>
  <% end %>
</div>